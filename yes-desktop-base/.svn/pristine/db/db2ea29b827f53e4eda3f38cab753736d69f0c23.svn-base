 @import "../bootstrap/variables";
 @import "../bootstrap/mixins";
 .jq-marquee {
     height: 250px;
     overflow: hidden;
 }
 
 .jq-marquee {
     input, button, select, textarea {
         outline: none;
     }
     ul, li, dl, ol {
         list-style: none;
         margin: 0;
         padding: 0;
     }
 }
 
 .jq-marquee li {
     padding: 10px;
 }
 
 .jq-marquee li.lieven {
     background: #e3f8ff;
 }
 
 .jq-marquee li p {
     height: 24px;
     line-height: 24px;
 }
 
 .jq-marquee li p a {
     float: left;
 }
 
 .jq-marquee li p em {
     width: 80px;
     font: normal 12px/24px Arial;
     /*color: #FF3300;*/
     float: left;
     display: inline-block;
 }
 
 .jq-marquee li p span {
     color: #e3f8ff;
     float: left;
 }
 
 .jq-marquee li p a.btn_lh {
     /*color: #28BD19;*/
     height: 17px;
     line-height: 17px;
     padding: 0 5px;
     margin-top: 4px;
     display: inline-block;
     float: left;
 }
 
 .btn_lh:hover {
     color: #fff;
     text-decoration: none;
 }