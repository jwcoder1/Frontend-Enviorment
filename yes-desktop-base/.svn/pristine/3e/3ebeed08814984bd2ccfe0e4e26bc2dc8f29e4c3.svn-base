<div ng-controller="single">
    <div class="page-content">
        <form name="base_form" class="base-form" ng-submit="action.save($event)">

            <div class="buttons-wrap">
                <button class="btn btn-sm btn-primary" type="submit">
                    <i class="glyphicon glyphicon-save "></i> {{'保存' | translate}}
                </button>
                <button class="btn btn-sm btn-primary" type="reset" ng-click="action.cancel();">
                    <i class="ace-icon fa fa-undo bigger-110"></i>{{'重置' | translate}}
                </button>
                <button class="btn btn-sm btn-primary" type="button" ng-click="action.back();">
                    <i class="ace-icon fa fa-undo bigger-110"></i>{{'返回' | translate}}
                </button>
            </div>

            <div sf-schema="detail.schema" sf-form="detail.form" sf-model="detail.model"></div>
        </form>
    </div>
</div>
