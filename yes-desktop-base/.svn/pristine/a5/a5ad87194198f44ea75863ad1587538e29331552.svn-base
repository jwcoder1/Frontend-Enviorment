<div>
    <div class="widget-body">
        <form name="edit$form" class="schema-form" role="form"
              ng-submit="save(edit$form)">
              <label style="  margin-bottom: 0;margin-top: 3px;">创建群组</label>
            <div class="buttons-wrap">
                <button type="submit" name="yes_temp_show_task" class="btn btn-sm btn-primary"
                        title=""><i class="fa fa-save bigger-110"></i> 保存
                </button>
                <div class="base-form selector-form clearfix">

                    <div class="form-group col-xs-12 col-sm-12">
                        <div class="col caption">
                            <label class="control-label">
                                {{'编号' | translate}}
                            </label>
                        </div>
                        <div class="col content">
                            <input type="text" class="form-control" ng-model="model.gid" readonly="readonly" placeholder="编号自动生成">
                        </div>
                    </div>

                    <div class="form-group col-xs-12 col-sm-12">
                        <div class="col caption">
                            <label class="control-label">
                                {{'业务编号' | translate}}
                            </label>
                        </div>
                        <div class="col content">
                            <input type="text" class="form-control" ng-model="model.myid" maxLength="32">
                        </div>
                    </div>

                    <div class="form-group col-xs-12 col-sm-12">
                        <div class="col caption">
                            <label class="control-label required">
                                {{'名称' | translate}}
                            </label>
                        </div>
                        <div class="col content">
                            <input type="text" class="form-control" ng-model="model.name" maxLength="64">
                        </div>
                    </div>

                    <div class="form-group col-xs-12 col-sm-12">
                        <div class="col caption">
                            <label class="control-label">
                                {{'描述' | translate}}
                            </label>
                        </div>
                        <div class="col content">
                            <input type="text" class="form-control" ng-model="model.describe" maxLength="128">
                        </div>
                    </div>

                </div>
            </div>
        </form>
    </div>
</div>