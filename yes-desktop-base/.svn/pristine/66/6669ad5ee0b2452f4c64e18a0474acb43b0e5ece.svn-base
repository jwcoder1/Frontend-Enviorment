.clickable {
  cursor: pointer;
}

.top-div{
  overflow: hidden;
}

.my-list {
  border: 1px solid #d4d4d4;

  .my-table-title {
    background: -webkit-gradient(linear, left bottom, left top, color-stop(0, #eee), color-stop(1, #fff));
    line-height: 25px;
    height: 25px;
    font-size: 14px;
    border-bottom: 1px solid #d4d4d4;
    & > div {
      float: left;
      text-align: center;
      margin: 0;
      &:not(:first-child) {
        border-left: 1px solid #d4d4d4;
      }
    }
  }

  .my-table-item {
    &:nth-child(odd) {
      background-color: #fdfdfd;
    }
    &:nth-child(even) {
      background-color: #f3f3f3;
    }
    & > div {
      float: left;
      overflow: hidden;
      white-space: nowrap;
      text-overflow: ellipsis;
      margin: 0;
      text-align: center;
    }
    & > div:not(:first-child) {
      border-left: 1px solid #d4d4d4;
    }
  }

  .page-div {
    width: 100%;
    text-align: center;
    .curePage {
      font-size: 15px;
    }
    .page-icon {
      font-size: 20px;
      padding: 0 6px;
      cursor: pointer;
    }
  }

  & > .my-title {
    line-height: 35px;
    height: 35px;
    border-bottom: 1px solid #B8B1B1;
    text-align: center;
    font-size: 16px;
    button {
      margin-bottom: 5px;
    }
  }

  .my-item {
    line-height: 25px;
    height: 25px;
    font-size: 12px;
    &.current {
      background-color: #2a88be !important;
      color: #fff !important;
    }
    .hover-show {
      display: none;
    }
    &:hover {
      .hover-show {
        display: inline !important;
      }
      background-color: rgba(98, 168, 209, 0.1);
    }
  }

}

.operate-span {
  padding: 2px 3px;
}

.operate-span-edit {
  padding: 2px 1px;
}

.operate-span:hover {
  color: red;
}

.operate-span-edit:hover {
  color: red;
}

.add-btn {
  font-size: 16px;
  color: #06ca06;
}

.search-items > .item {
  width: 33% !important;
}

.dialog-people-selector {
  .tree .checked {
    background-color: #f0f7fc;
  }

  .tree .operate-span {
    padding: 2px 4px;
  }

  .tree .operate-btn.fa-plus-circle {
    font-size: 14px !important;
    color: #06ca06;
  }

  .tree .operate-btn.fa-minus-circle {
    font-size: 14px !important;
    color: #c71313;
  }

  .tree .tree-branch {
    cursor: auto !important;
  }

  .tree-small .tree-branch .tree-branch-children {
    margin: 0 0 0 17px !important;
  }

  .tree-small:before {
    bottom: 12px !important;
  }

  .tree-small .tree-branch:before,
  .tree-small .tree-item:before {
    top: 10px !important;
    left: -14px !important;
  }

  .tree-small .tree-branch .tree-branch-children:before {
    left: -13px !important;
    bottom: 12px !important;
  }

  .tree-small .tree-item,
  .tree-small .tree-branch .tree-branch-header {
    padding: 1px !important;
  }

  .tree-small div {
    font-size: 12px !important;
  }

  .tree-children {
    padding-left: 20px;
  }

  .tree-item-name,
  .tree-branch-name {
    white-space: nowrap !important;
  }

  padding-bottom: 0 !important;
  padding-top: 47px !important;
  .ngdialog-content {
    background: #fff !important;
    width: 1000px !important;
  }
}

.my-organization-edit .ngdialog-content {
  width: 600px !important;
}

.document-selector {
  .btn-bottom {
    margin: 0;
    padding: 0;
  }
  .btn-bottom .btn {
    margin-top: 4px;
    float: right;
    padding: 3px 5px;
  }
}

.document-selector .checkbox-label {
  input {
    margin: 2px 0;
    width: 18px;
    height: 18px;
    position: absolute;
  }
  span {
    font-weight: 400;
    font-size: 14px;
    margin-left: 20px;
    cursor: pointer;
  }
}

.document-selector {
  div {
    font-size: 14px;
  }
  a {
    color: #337ab7;
    text-decoration: none;
    background-color: transparent;
  }
  a:hover, a:focus {
    color: #23527c;
    text-decoration: underline !important;
  }
  a:active, a:hover {
    outline: 0;
  }
}

.sift-div {
  width: 25px;
  float: left;
  font-family: Arial, "微软雅黑", "宋体";
  font-size: 14px;
  line-height: 18px;
  color: #333333;
  text-decoration: none;
  text-align: center;
}

.sift-div .characters-ul {
  list-style: none;
  margin: 0;
  padding: 0;
}

.sift-div .characters-ul > li {
  line-height: 17px;
  color: #6fb3e0;
  font-size: 14px;
  cursor: pointer;
}

.sift-div .characters-ul > li:hover {
  color: #fff;
  background-color: #6fb3e0;
}

.document-selector .top-div {
  height: 43px;
  width: 100%;
}

.document-selector .tree-div {
  font-family: Verdana, Arial, Helvetica, AppleGothic, sans-serif !important;
  width: auto;
  height: 442px;
  overflow-y: auto;
  border: 1px solid #DCE8F1;
}

.document-selector .tree-div .person_girl {
  background-position: left 50%;
  background-image: url(../images/woman.gif);
  background-repeat: no-repeat;
}

.document-selector .tree-div .person_man {
  background-image: url(../images/man.gif);
  background-position: left 50%;
  background-repeat: no-repeat;
}

.document-selector .tree-div .person_dept {
  background-image: url(../images/dept.gif);
  background-position: left 50%;
  background-repeat: no-repeat;
}

.document-selector .tree-div .people-row:hover {
  background-color: #F0F7FC;
}

.document-selector .tree-div .people-row {
  border-bottom: 1px solid #CCCCCC;
  color: #333333;
  text-decoration: none;
  text-indent: 20px;
  height: 27px;
  padding: 3px 5px 0 0;
  cursor: pointer;
  font-size: 14px;
  background-repeat: no-repeat;
}

.document-selector .tree-div .people-row > i {
  text-indent: 5px;
  float: right;
  margin-top: 3px;
}

.document-selector .tree-div .people-row > i.fa-minus {
   color: #FF0000;
}

.document-selector .tree-div .people-row > i.fa-plus {
  color: #FC0;
}

.document-selector .tree-div .people-row > .right {
  text-indent: 0px;
  display: block;
  width: 170px;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  float: right;
}

.document-selector .page-div {
  width: 100%;
  text-align: center;
}

.document-selector .page-div .curePage {
  font-size: 15px;
}

.document-selector .page-div .page-icon {
  font-size: 20px;
  padding: 0 6px;
  cursor: pointer;
}

.search-span {
  width: 100%;
}

.search-span > input {
  height: 29px;
  width: 100%;
  border: 1px solid #ddd;
}

.enterprise-selector {
  padding-top: 3px;
  padding-bottom: 3px;
  border-bottom: 1px solid #B8B1B1;
